<?xml version="1.0"?>
<extension key="imgoutputbuffer" type="module">
  <file>imgoutputbuffer</file>
  <name>Clean Output Buffers</name>
  <description>Clean any output buffers when dealing with image display/download in civicrm.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Deepak Srivastava</author>
    <email>deepak@mountev.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mountev/imgoutputbuffer</url>
    <url desc="Documentation">https://github.com/mountev/imgoutputbuffer</url>
    <url desc="Support">info@mountev.co.uk</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-09-08</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Imgoutputbuffer</namespace>
  </civix>
</extension>
